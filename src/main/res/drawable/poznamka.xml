<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:bottom="20dp"
        android:right="20dp">
        <shape
            android:shape="rectangle">
            <stroke android:width="1dp" android:color="@color/poznamka" />
            <solid android:color="@color/poznamka" />
            <padding android:bottom="1dp"/>
            <size android:width="100dp"
                android:height="100dp"/>
        </shape>
    </item>
    <item android:left="90dp"
        android:top="90dp"
        android:bottom="25dp"
        android:right="25dp">
        <shape
            android:shape="oval">
            <stroke android:width="1dp" android:color="@color/colorPrimary" />
            <solid android:color="@color/colorPrimary" />
            <size android:width="2dp"
                android:height="2dp"/>
        </shape>
    </item>
    <item android:left="5dp"
        android:top="90dp"
        android:bottom="25dp"
        android:right="110dp">
        <shape
            android:shape="oval">
            <stroke android:width="1dp" android:color="@color/colorPrimary" />
            <solid android:color="@color/colorPrimary" />
            <size android:width="2dp"
                android:height="2dp"/>
        </shape>
    </item>
    <item android:left="5dp"
        android:top="5dp"
        android:bottom="110dp"
        android:right="110dp">
        <shape
            android:shape="oval">
            <stroke android:width="1dp" android:color="@color/colorPrimary" />
            <solid android:color="@color/colorPrimary" />
            <size android:width="2dp"
                android:height="2dp"/>
        </shape>
    </item>
    <item android:left="90dp"
        android:top="5dp"
        android:bottom="110dp"
        android:right="25dp">
        <shape
            android:shape="oval">
            <stroke android:width="1dp" android:color="@color/colorPrimary" />
            <solid android:color="@color/colorPrimary" />
            <size android:width="2dp"
                android:height="2dp"/>
        </shape>
    </item>

</layer-list>